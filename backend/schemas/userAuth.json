{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/userAuth.json",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength": 25
      },
      "password": {
        "type": "string",
        "minLength": 5
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "password"
    ]
  }